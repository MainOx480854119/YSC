{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "launch bot",
      "skipFiles": ["<node_internals>/**"],
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "pnpm: build",
      "runtimeExecutable": "pnpm",
      "args": ["start"],
      "outFiles": ["${workspaceFolder}/dist/*.js", "!**/node_modules/**"]
    }
  ]
}
